{
  "name": "cf-jwt-auth",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev --name cf-jwt-auth --port 7787 --inspector-port 7329  --persist-to=.db-local",
    "deploy:staging": "wrangler deploy --env staging",
    "start": "wrangler dev",
    "test": "vitest",
    "cf-typegen": "wrangler types",
    "migrate": "./migrate.sh",
    "generate": "graphql-codegen",
    "migration-list": "wrangler d1 migrations list DB",
    "migration-apply-production": "wrangler d1 migrations apply DB --remote",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,mts,d.ts}\" --no-error-on-unmatched-pattern",
    "prepare": "husky"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.5.2",
    "@cloudflare/workers-types": "^4.20241230.0",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typescript": "^4.1.2",
    "@graphql-codegen/typescript-resolvers": "^4.4.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "typescript": "^5.5.2",
    "vitest": "2.1.8",
    "wrangler": "^3.60.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.3",
    "gql": "^1.1.2",
    "graphql": "^16.10.0",
    "graphql-yoga": "^5.10.9",
    "husky": "^9.1.7",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.9"
  }
}
